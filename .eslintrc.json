{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars":"warn",
    "import/no-extraneous-dependencies":"off",
    "lines-between-class-members":"off",
    "no-param-reassign":"off",
    "prefer-destructuring":"off",
    "no-nested-ternary":"off",
    "no-alert":"off"
  }
}
